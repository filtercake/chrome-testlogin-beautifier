{
  "manifest_version": 2,
  "name": "test login beautifier",
  "description": "This extension beautifies the test login page",
  "background": { "scripts": ["background.js"] },
  "version": "0.1",
  "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "unlimitedStorage",
      "http://testserver/"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "test logins"
  },
  "icons" : {
      "48" : "icon.png",
      "128" : "icon.png"
    }
}
